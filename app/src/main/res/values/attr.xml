<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="journeyToolbarStyle" format="reference"/>
    <attr name="journeyTextInputLayoutTextColor" format="reference"/>
    <attr name="journeyTextInputLayoutTextColorHint" format="reference"/>

    <declare-styleable name="SuffixEditText">
        <attr name="suffix" format="string"/>
    </declare-styleable>

    <declare-styleable name="ErrorState">
        <attr name="state_error" format="boolean" />
        <attr name="state_empty" format="boolean" />
    </declare-styleable>

    <declare-styleable name="JourneyFloatingEditText">
        <attr name="android:hint"/>
        <attr name="android:inputType"/>
        <attr name="android:imeOptions"/>
        <attr name="android:maxLength"/>
        <attr name="android:maxLines"/>
        <attr name="hintTextAppearance" format="reference"/>
        <attr name="errorTextAppearance" format="reference"/>
    </declare-styleable>
</resources>